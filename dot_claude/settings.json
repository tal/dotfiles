{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(ls:*)",
      "Bash(find:*)",
      "Bash(rg:*)",
      "Bash(grep:*)",
      "Bash(sed:*)",
      "Bash(awk:*)",
      "Bash(cat:*)",
      "Bash(defaults read:*)",
      "Bash(mdfind:*)",
      "Bash(git fetch:*)",
      "Bash(git pull:*)",
      "Bash(git status:*)",
      "Bash(git log:*)",
      "Bash(git diff:*)",
      "Bash(git branch:*)",
      "Bash(git show:*)",
      "Bash(git rev-parse:*)",
      "Bash(git add:*)"
    ],
    "ask": [
      "Bash(rm:*)",
      "Bash(mv:*)",
      "Bash(git push:*)",
      "Bash(git commit:*)",
      "Bash(git checkout:*)",
      "Bash(git merge:*)",
      "Bash(git rebase:*)"
    ],
    "additionalDirectories": ["/tmp/"]
  },
  "hooks": {
    "Notification": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "bash ~/.claude/hooks/notify.sh",
            "timeout": 5
          }
        ]
      }
    ]
  },
  "env": {
    "ENABLE_TOOL_SEARCH": "true"
  },
  "enabledPlugins": {
    "dd@datadog-claude-plugins": true,
    "plugin-dev@claude-plugins-official": true,
    "ralph-wiggum@claude-plugins-official": true,
    "swift-lsp@claude-plugins-official": true
  },
  "alwaysThinkingEnabled": true,
  "feedbackSurveyState": {
    "lastShownTime": 1754080668867
  },
  "statusLine": {
    "type": "command",
    "command": "/Users/tal.atlas/.claude/statusline-command.sh"
  }
}
